.subscribePage{background-color: #f5f5f5; min-height: 100vh;}
  .block{margin-bottom: 20rpx;background-color: #fff;padding-left: 20rpx;}
  .item{padding-right: 20rpx;display: flex;justify-content: space-between;align-items: center;height: 100rpx;font-size: 28rpx;line-height: 40rpx;}
    .label{color: #1E2328;width: 150rpx;}
      .rightArrow{height: 32rpx;width: 32rpx;}
    .labelValue{color: #73737D;display: flex;align-items: center;}
      .picker{display: flex;align-items: center;}
  .shop{display: flex;padding: 30rpx 20rpx 30rpx 0;}
    .shopImg{height: 200rpx;width: 200rpx;margin-right: 20rpx;}
    .shopInfo{width: 490rpx; display: flex;flex-direction: column;justify-content: space-between;}
      .shopTitle{color: #1E2328;font-size: 32rpx;line-height: 44rpx;}
      .shopSubTitle{color: #73737D;font-size: 24rpx;line-height: 40rpx;}
      .icon{width:33%;float:left;font-size:25rpx;color:RGBA(115,115,125,1);line-height:40rpx;display: flex;align-items: center;}
        .icon image{width:35rpx;height:35rpx;margin-right: 8rpx;}
  .borderTop{border-top: 1rpx solid #E6E6EB;}
  .buttonBlock{height: 120rpx;width: 100%;display: flex;justify-content: center;align-items: center; position: fixed;bottom: 0;background-color: #fff;border-top: 1rpx solid #ECEDF1;} 
    .btn{width: 520rpx;height: 94rpx;}



button::after,.button-hover{border-radius: 0;border: 0;outline: none;}
button[disabled][type="default"],wx-button[disabled]:not([type]) {background: #C8C8D2;color: #fff;}
button{color: #fff;background-color: #1ea0ff;border-radius: 0;outline: none;display: flex;align-items: center;justify-content: center;}

.text2Row{overflow:hidden; text-overflow:ellipsis;display:-webkit-box; -webkit-box-orient:vertical;-webkit-line-clamp:2; }
.text1Row{overflow:hidden; text-overflow:ellipsis;display:-webkit-box; -webkit-box-orient:vertical;-webkit-line-clamp:1 }
