.imgBox{
  width: 150rpx;
  height: 200rpx;
  float: left;
  
}
.img{
  width: 150rpx;
  height: 150rpx;
  color:RGBA(200, 200, 210, 1);
  text-align: center;
  line-height: 130rpx;
  border: 1rpx dashed RGBA(150, 155, 165, 1);
}
.img text{
  color:RGBA(200, 200, 210, 1);
  
  font-size: 100rpx;
}
.img image{
  width: 100%;
  height: 100%;
}
.imgTxt{
  width: 100%;
  margin-top:30rpx; 
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: RGBA(30, 35, 40, 1);
  /* background: red; */
  font-size: 30rpx;
}
.btn{
  width: 60%;
  height: 80rpx;
  color: white;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius:0rpx;
  background: RGBA(30, 160, 255, 1); 
  margin: 70rpx;
}
.Photo{
  display: flex;
  justify-content: space-between
}
.Photo>view{
  width: 116rpx;
  height: 116rpx;
  
}
.Photo>view>image{
  width: 100%;
  height: 100%;
}
.Submission{
  width: 100%;
  text-align: center;
  display: flex;
  justify-content: center
}
