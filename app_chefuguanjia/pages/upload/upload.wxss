.imgBox {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.example .img {
  width: 100% !important;
  height: 100% !important;
}

.example .imgTxt {
  color: #73737d !important;
  font-size: 20rpx !important;
  margin-bottom: 40rpx;
}

.img {
  width: 116rpx;
  height: 116rpx;
  color: RGBA(200, 200, 210, 1);
  text-align: center;
  line-height: 100rpx;
  border: 1rpx dashed RGBA(150, 155, 165, 1);
}

.img text {
  color: RGBA(200, 200, 210, 1);
  font-size: 100rpx;
}

.img image {
  width: 100%;
  height: 100%;
}

.imgTxt {
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: RGBA(30, 35, 40, 1);
  font-size: 20rpx;
}

.example {
  width: 100%;
  margin-top: 80rpx;
  border: 1rpx solid RGBA(240, 240, 240, 1);
  padding: 36rpx 36rpx;
  position: relative;
}

.btn {
  width: 60%;
  height: 80rpx;
  background: RGBA(30, 160, 255, 1);
  border-radius: 0rpx;
  line-height: 80rpx;
  color: white;
  margin-top: 50rpx;
}

.Photo {
  display: flex;
  justify-content: space-between;
  text-align: center;
  font-size: 20rpx;
  color: #73737d;
}

.Photo>view>view:last-child {
  padding: 10rpx 0;
}

.Photo_img {
  width: 116rpx;
  height: 116rpx;
}

.Photo_img>image {
  width: 100%;
  height: 100%;
  border: 1rpx solid #969ba5;
}

.Tips_img {
  font-size: 28rpx;
  color: red;
  line-height: 40rpx;
  margin: 20rpx 0 0 20rpx;
}

.primary_img {
  position: fixed;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 11;
  top: 0;
  width: 100%;
  height: 100%;
}

.primary_img>view {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  text-align: center;
  justify-content: center;
}

.tiem>view {
  width: 100%;
  height: 60rpx;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  font-size: 35rpx;
  justify-content: space-between;
}

.tiem>view:last-child {
  margin-bottom: 20rpx;
}

.picker {
  display: flex;
  align-items: center;
  justify-content: space-between;
  text-align: right;
}

.picker>text {
  flex: 1;
}

.tiem>view image {
  width: 50rpx;
  height: 50rpx;
}

