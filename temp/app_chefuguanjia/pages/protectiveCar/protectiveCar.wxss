/* pages/protectiveCar.wxss */
.protectiveCarPage{background-color: #F5F5F5;min-height: 100vh;width: 750rpx;}
  .listBox{padding-bottom: 200rpx;}
    .listItem{background-color: #fff;padding-left: 30rpx;margin-bottom: 20rpx;}
      .itemHeader{height: 88rpx;padding-right: 30rpx;display: flex; align-items: center; }
        .itemTitle{color: #73737D;font-size: 28rpx;} 
      .itemBody{border-top: 1px solid #E6E6EB; border-bottom: 1px solid #E6E6EB;padding: 26rpx 0 52rpx 0; min-height: 102rpx;}
        .tipTitle{color: #1E2328;font-size: 24rpx;display: block;height:40rpx;}
        .tipContent{margin-top: 12rpx;display: block;color: #1E2328;font-size: 24rpx;line-height: 40rpx;}
      .itemFooter{height: 90rpx;display: flex;align-items: center;justify-content: space-between;}
        .footerBox{height:30rpx;color: #73737D;font-size: 24rpx;width: 100%;padding-right: 46rpx;} 
.tipTime{
  width: 230rpx;
  height: 30rpx;
  float: right;
}

.emptyBox{min-height: 100vh;display: flex;flex-direction: column;justify-content: center;align-items: center;padding-bottom: 50rpx}
.wufap{font-size:28rpx;width: 300rpx;text-align: center;font-weight:400;color:rgba(115,115,125,1);line-height:40rpx;margin-top: 20rpx;}