.banner {
  width: 100%;
  position: relative;
}

.bg {
  width: 100%;
  height: 680rpx;
  position: absolute;
  top: 0;
  z-index: -1;
}

.fcc {
  width: 220rpx;
  height: 50rpx;
  font-size: 25rpx;
  color: white;
  margin: -104rpx auto;
  text-align: center;
  position: absolute;
}

.Title {
  width: 100%;
  height: 80rpx;
  padding-top: 40rpx;
  line-height: 40rpx;
  text-align: center;
  color: RGBA(255, 225, 135, 1);
  font-weight: 600;
}

.item {
  width: 100%;
  height: 270rpx;
  background-size: 710rpx 260rpx;
  background-position: center;
  background-repeat: no-repeat;
  display: flex;
  margin: 20rpx 0;
  align-items: center;
  padding: 0 20rpx;
  justify-content: space-around;
  position: relative;
}

.items {
  width: 150rpx;
  height: 200rpx;
  margin-top: 30rpx;
}

.itemsImg image {
  width: 90rpx;
  height: 90rpx;
  border-radius: 45rpx;
  margin-left: 30rpx;
}

.itemsFc {
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  color: white;
  text-align: center;
  font-size: 30rpx;
}

.itemsFc2 {
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  color: RGBA(185, 224, 255, 1);
  text-align: center;
  font-size: 17rpx;
}

.price {
  width: 95%;
  height: 300rpx;
  background: white;
  border-radius: 15rpx;
  bottom: -250rpx;
  margin: auto;
  box-shadow: 0px 2px 2px 2px RGBA(230, 230, 230, 1);
}

.priceText {
  width: 100%;
  height: 110rpx;
  line-height: 110rpx;
  text-align: center;
  font-size: 35rpx;
  position: relative;
  color: RGBA(30, 35, 40, 1);
}

.isd {
  color: RGBA(250, 100, 75, 1);
  text-align: center;
  font-size: 25rpx;
  margin-top: 20rpx;
}

.priceText image {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  top: 0rpx;
  right: 0rpx;
}

.line {
  width: 200rpx;
  height: 2rpx;
  background: black;
  margin: auto;
}

.num {
  width: 100%;
  height: 100rpx;
  padding: 20rpx;
  line-height: 60rpx;
  font-size: 60rpx;
  text-align: center;
  font-weight: bold;
  color: RGBA(255, 211, 82, 1);
}

.fc {
  font-size: 25rpx;
  color: RGBA(115, 115, 125, 1);
  text-align: center;
}

.zhekouBox {
  width: 95%;
  height: 180rpx;
  margin: 100rpx auto;
  position: relative;
}

.zhekouBox image {
  width: 100%;
  height: 100%;
}

.txt {
  width: 100%;
  height: 80rpx;
  position: absolute;
  top: 0rpx;
  margin-top: 40rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 32rpx;
}

.rc {
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  position: absolute;
  bottom: 20rpx;
  text-align: center;
  color: RGBA(30, 160, 255, 1);
}

.tc {
  position: absolute;
  width: 150rpx;
  height: 40rpx;
  top: -15rpx;
  font-size: 28rpx;
  left: 0rpx;
  right: 0rpx;
  text-align: center;
  margin: auto;
  color: RGBA(30, 160, 255, 1);
}

.zc1 {
  line-height: 70rpx;
  padding: 10rpx 30rpx 10rpx 30rpx;
}

.zc2 {
  line-height: 50rpx;
  padding: 50rpx 50rpx;
}

.sanjiao {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  left: 0rpx;
  right: 0rpx;
  margin: auto;
}

.ConfirmBuy {
  width: 70%;
  height: 90rpx;
  background: linear-gradient(RGBA(255, 235, 138, 1), RGBA(255, 211, 82, 1));
  border-radius: 45rpx;
  margin: auto;
}

.ConfirmBuy {
  line-height: 90rpx;
  text-align: center;
  color: RGBA(110, 95, 20, 1);
  font-weight: 600;
  margin-top: 30rpx;
}

