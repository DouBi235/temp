/* 信息弹窗 */
.infoshow{position: fixed;top: 0;height: 100vh;width: 100vw;background: rgba(0, 0, 0, 0.3)}
.infoWhite{width:500rpx;height:600rpx;background:rgba(255,255,255,1);border-radius:10rpx;position: absolute;top: 300rpx;left: 0;right: 0;margin:0 auto;z-index: 5000}
.infomain{height: 490rpx;display: flex;flex-direction: column;justify-content: space-around;align-items: center;padding: 50rpx 0;box-sizing: border-box;}
.infoImg{height: 174rpx;width: 174rpx;border-radius: 50%;}
.infoname{font-size:36rpx;font-family:PingFangSC-Regular;font-weight:400;color:rgba(30,35,40,1);line-height:50rpx;}
.infotip {font-size:28rpx;font-family:PingFangSC-Regular;font-weight:400;color:rgba(30,35,40,1);line-height:40rpx;}
.infobtnbox{height: 108rpx;width: 100%;font-size:32rpx;font-family:PingFangSC-Regular;font-weight:400;color: #1E2328;display: flex;text-align: center;}
.infoconfirm{width: 250rpx;height: 100%;line-height:108rpx;color: #1EA0FF;}
.infocancel{width: 250rpx;height: 100%;line-height:108rpx;}
.infoconfirm:active{background: #ccc}
.infocancel:active{background: #ccc}
