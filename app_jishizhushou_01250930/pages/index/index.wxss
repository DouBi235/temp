page {
  background: #f5f5f5;
}

image {
  display: block;
}

.banner, .slide-image {
  width: 750rpx;
  height: 340rpx;
  background: #ccc;
}

.userinfo {
  width: 690rpx;
  height: 162rpx;
  box-sizing: border-box;
  position: absolute;
  bottom: -80rpx;
  left: 30rpx;
  background: #fff;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  padding: 0 30rpx;
  box-shadow: 0px 0px 28rpx rgba(14, 111, 255, 0.5);
}

.userhead {
  position: relative;
  width: 137rpx;
  height: 122rpx;
  margin-left: 30rpx;
}

.userhead>image {
  position: absolute;
}

.headimg {
  top: 0;
  width: 110rpx;
  height: 110rpx;
  left: 13.5rpx;
  z-index: 5;
  border-radius: 200rpx;
  background: #ccc;
}

.nametel {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 80rpx;
  color: #333;
  font-size: 28rpx;
  margin-left: 30rpx;
}

.nametel>text {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  max-width: 400rpx;
}

.unloadimg {
  width: 122rpx;
  height: 122rpx;
  border-radius: 200rpx;
}

.unloadtip {
  color: #333;
  font-size: 30rpx;
  margin-left: 30rpx;
}

.tab-h {
  position: absolute;
  bottom: 0;
  left: 0;
  height: 80rpx;
  width: 100%;
  font-size: 32rpx;
  color: #0e6fff;
  display: flex;
  align-items: center;
}

.tab-label {
  width: 4rpx;
  height: 34rpx;
  background: #0e6fff;
  border-radius: 8rpx;
  margin-left: 30rpx;
  margin-right: 20rpx;
}

.content {
  padding-top: 4rpx;
  padding-bottom: 160rpx;
  background: #f5f5f5;
}

.nomore {
  width: 750rpx;
  height: 60rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 24rpx;
  color: #aaa;
}

.line {
  width: 160rpx;
  height: 2rpx;
  background: #ccc;
}

text.r {
  color: #f00;
}

