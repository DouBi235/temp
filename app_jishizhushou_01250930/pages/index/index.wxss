.xxxx {
  width: 140rpx;
  height: 140rpx;
  position: fixed;
  z-index: 122;
  right: 10rpx;
  bottom: 34rpx;
  border-radius: 150rpx;
}

.text {
  width: 460rpx;
  height: 200rpx;
  position: relative;
}

.title {
  font-size: 26rpx;
  color: #333;
  width: 460rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-bottom: 28rpx;
}

.tip {
  font-size: 24rpx;
  color: #666;
}

.uper {
  margin-right: 28rpx;
}

.date {
  position: absolute;
  bottom: 0;
  left: 0;
  font-size: 22rpx;
  color: #999;
}

.float {
  position: fixed;
  z-index: 159;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(255, 255, 255, 0.5);
}

.floatborder {
  width: 520rpx;
  height: 427rpx;
  position: absolute;
  left: calc(50% - 260rpx);
  top: calc(50% - 212rpx);
  z-index: 169;
}

.floaticon {
  width: 65rpx;
  height: 53rpx;
  position: absolute;
  left: calc(50% - 30rpx);
  top: calc(50% - 172rpx);
  z-index: 179;
}

.fltext {
  width: 400rpx;
  height: 300rpx;
  position: absolute;
  z-index: 189;
  left: 178rpx;
  top: calc(50% - 146rpx);
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  color: #fff;
  font-size: 30rpx;
  padding: 100rpx 0 30rpx;
  box-sizing: border-box;
  letter-spacing: 2rpx;
}

.flb {
  font-size: 48rpx;
  font-weight: bold;
  letter-spacing: 4rpx;
  color: #fff;
  width: 360rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 0;
  background: transparent;
}

.warp {
  color: #333;
  font-size: 30rpx;
  letter-spacing: 4rpx;
  height: calc(100vh - 340rpx);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
}

.t1 {
  text-align: center;
}

image {
  display: block;
  width: 324rpx;
  height: 324rpx;
  margin: 0 auto;
}

.box {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 100rpx 0;
  font-size: 26rpx;
}

view {
