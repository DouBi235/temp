}

.box>input {
  font-size: 28rpx;
  color: #333;
  width: 310rpx;
  background: #f5f5f5;
  outline: none;
  margin-top: 28rpx;
  border: solid #ccc;
  border-width: 0 0 2rpx 0;
}

.search {
  color: #3de7ff;
  font-size: 30rpx;
  text-align: center;
  margin-top: 100rpx;
  text-decoration: underline;
}

.boxr {
  color: #666;
