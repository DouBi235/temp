}

.imgborder {
  width: 240rpx;
  height: 240rpx;
  border: dashed 2rpx #ccc;
  display: flex;
  justify-content: center;
  align-items: center;
}

.imgborder>image {
  width: 100%;
  height: 100%;
  display: block;
}

.imgborder>image.camera {
  width: 106rpx;
  height: 86rpx;
  margin-top: 32rpx;
}

.imgtip {
  color: #333;
  margin-top: 60rpx;
}

.puttip {
  width: 310rpx;
